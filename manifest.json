{
  "name":             "GeoGuessrNim",
  "short_name":       "GeoGuessr",
  "version":          "0.1",
  "manifest_version": 2,
  "description":      "GeoGuessr 'helper' plugin written in Nim",
  "author":           "Juan_Carlos",
  "homepage_url":     "https://github.com/juancarlospaco",
  "offline_enabled":  false,
  "content_scripts":  [{
    "matches":        ["https://www.geoguessr.com/game/*"],
    "js":             ["index.js"]
  }],
  "permissions":      ["tabs", "experimental", "geolocation"],
  "icons": {
    "16":             "icon16.png",
    "32":             "icon32.png",
    "48":             "icon48.png",
    "128":            "icon128.png"
  }
}
